generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Question {
  id       Int    @id @default(autoincrement())
  question String
  answer   String
}

model UnansweredQuestion {
  id        Int      @id @default(autoincrement())
  question  String
  createdAt DateTime @default(now())
}
